<h1 class="bg-success-subtle p-2 text-center text-success w-100">EVENTS</h1>
<section class="d-flex flex-wrap justify-content-evenly">
  {{#each events}}
    <div class="card m-1" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title">BIRTHDAY EVENT</h5>
        <p class="card-text">Created by: {{this.user_id.name}}</p>
        <p class="card-text">Place: {{this.place}}</p>
      </div>
    </div>
  {{/each}}
</section>